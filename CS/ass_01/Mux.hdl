/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Mux=Or(And(Not(sel),a),And(sel,b))
    Not(in=sel,out=nsel);
    And(a=nsel,b=a,out=c);
    And(a=sel,b=b,out=d);
    Or(a=c,b=d,out=out);
}
