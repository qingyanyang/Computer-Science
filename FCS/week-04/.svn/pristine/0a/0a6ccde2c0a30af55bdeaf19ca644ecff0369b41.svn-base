class Turtle extends Animal {
    String species;
    //defualt constructor
    public Turtle(){
        super();
        species = "Turtle";
    }
    //constructor with parameters
    public Turtle(String species){
        super(30);
        this.species = species;
        System.out.println("species: " + this.species);
    }

    @Override
	public void makeSound(){
		System.out.println("my sound: bobobobo");
	}
}