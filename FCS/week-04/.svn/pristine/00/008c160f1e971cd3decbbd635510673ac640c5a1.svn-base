class Doctor extends Person{
    String speciality, type;
    //defualt constructor
    public Doctor(){
        super();
        speciality = "dental";
        type = "doctor";
    }
    //create constructor with parameters and call super class constructor
    public Doctor(String type, String speciality){
        super(28);
        this.type = type;
        this.speciality = speciality;
        System.out.println("type: " + this.type);
    }
}