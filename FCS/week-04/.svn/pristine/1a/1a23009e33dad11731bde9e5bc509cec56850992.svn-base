class Shape{
	double width, height, radius, length;

	//constructor without parameters
	public Shape(){
		width = 0.0d;
		height = 0.0d;
		radius = 0.0d;
		length = 0.0d;
	}
	//constructor with parameters
	public Shape(double radius){
		this();
		this.radius = radius;
	}

	//Accessor to get width value
	public double getWidth(){
		return this.width;
	}
	//Accessor to get height value
	public double getHeight(){
		return this.height;
	}
	//Accessor to get radius value
	public double getRadius(){
		return this.radius;
	}
	//Accessor to get length value
	public double getLength(){
		return this.length;
	}
	//Mutator to set width value
	public void setWidth(double width){
		this.width = width;
	}
	//Mutator to set height value
	public void setHeight(double height){
		this.height = height;
	}
	//Mutator to set radius value
	public void setRadius(double radius){
		this.radius = radius;
	}
	//Mutator to set length value
	public void setLength(double length){
		this.length = length;
	}

}