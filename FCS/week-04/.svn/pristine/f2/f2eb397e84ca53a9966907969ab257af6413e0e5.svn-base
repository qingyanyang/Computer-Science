class Person{
    String givenName, lastName, gender, citizenship;
	int age;

	//constructor without parameters
	public Person(){
        givenName = "unknown";
        lastName = "unknown";
        gender = "female";
        citizenship = "China";
        age = 27;
	}
	//constructor with parameters
	public Person(int age){
        this();
        this.age = age;
	}

	//create a method to display info
	public void display(){
		System.out.println("given name: " + this.givenName);
		System.out.println("last name: " + this.lastName);
		System.out.println("gender: " + this.gender);
		System.out.println("citizenship: " + this.citizenship);
		System.out.println("age: " + this.age);
	}

	//Accessor to get givenName value
	public String getGivenName(){
		return this.givenName;
	}
	//Accessor to get lastName value
	public String getLastName(){
		return this.lastName;
	}
	//Accessor to get gender value
	public String getGender(){
		return this.gender;
	}
	//Accessor to get citizenship value
	public String getCitizenship(){
		return this.citizenship;
	}
	//Accessor to get age value
	public int getAge(){
		return this.age;
	}
	//Mutator to set givenName value
	public void setGivenName(String givenName){
		this.givenName = givenName;
	}
	//Mutator to set lastName value
	public void setLastName(String lastName){
		this.lastName = lastName;
	}
	//Mutator to set gender value
	public void setGender(String gender){
		this.gender = gender;
	}
	//Mutator to set citizenship value
	public void setCitizenship(String citizenship){
		this.citizenship = citizenship;
	}
	//Mutator to set age value
	public void setAge(int age){
		this.age = age;
	}


}
