class Problem {
    public static void main(String[] args) {
	int i, evenNumber = 0;
	//print out [
	System.out.print("[ ");
	// screen even number between 10-30
        for(i = 10; evenNumber < 30; i++){
	    if(i % 2 == 0){
	    evenNumber = i;
	    //print out evennumber
	    System.out.print(evenNumber + ", ");
	    }	
	}
	System.out.println("]");// print out ]
    }
}
