class Player{
    private String move;
    public Player(){
        move = getMove();
    }
    public void produceMove(){
        this.move = getMove();
    }
    //method to display move
    public void performMove(){
        System.out.println("the move is: " + getMove());
    }
    //accessor to get move
    public String getMove(){
        return this.move;
    }
    //mutator to set move
    public void setMove(String move){
        this.move = move;
    }
}